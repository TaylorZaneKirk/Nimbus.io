<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="couldmsu.project.nimbusio">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotry"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SignUp"></activity>
        <activity android:name=".Home"></activity>
        <activity android:name=".ShowServers"></activity>
        <activity android:name=".AddServers"></activity>
        <activity android:name=".PresetServersList"></activity>
        <activity android:name=".ConfigureServer"></activity>
        <activity android:name=".PaymentInfo"></activity>
    </application>

</manifest>
